{
  "name": "linguist-languages",
  "version": "7.29.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "description": "Linguist languages.yml in JSON format",
  "keywords": [
    "data",
    "json",
    "language",
    "linguist"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./data/*": {
      "import": "./data/*.mjs",
      "require": "./data/*.js"
    }
  },
  "repository": "https://github.com/ikatyang-collab/linguist-languages",
  "homepage": "https://github.com/ikatyang-collab/linguist-languages#readme",
  "author": {
    "name": "Ika",
    "email": "ikatyang@gmail.com",
    "url": "https://github.com/ikatyang"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "22.13.9",
    "@vitest/coverage-v8": "3.0.8",
    "camelcase": "8.0.0",
    "jest-snapshot-serializer-raw": "2.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "vite": "6.2.1",
    "vite-node": "3.0.8",
    "vitest": "3.0.8",
    "yaml": "2.7.0"
  },
  "files": [
    "/lib/**/*",
    "/data/**/*",
    "/ThirdPartyNoticeText.txt"
  ],
  "scripts": {
    "lint": "prettier --check .",
    "check": "tsc --noEmit",
    "test": "vitest",
    "build": "vite-node ./scripts/generate.mts -- run"
  }
}